{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "services/*": ["services/*"],
      "components/*": ["components/*"],
      "useCases/*": ["useCases/*"],
      "models/*": ["models/*"],
      "utils/*": ["utils/*"],
    }
  }
}
